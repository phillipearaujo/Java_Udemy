package modelo.umpramuitos;

import java.util.Date;

public class Pedido {

	private Long id;
	
	private Date data;
	
	public Pedido() {
		// TODO Auto-generated constructor stub
	}

	public Pedido(Date date) {
		super();
		this.data = date;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Date getDate() {
		return data;
	}

	public void setDate(Date date) {
		this.data = date;
	}
	
	
	
	
}
